{
  "serviceName": "shadcn-admin-service",
  "cluster": "YOUR_CLUSTER_NAME",
  "taskDefinition": "shadcn-admin-app:1",
  "desiredCount": 2,
  "launchType": "FARGATE",
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": [
        "subnet-xxxxxxxxx",
        "subnet-yyyyyyyyy"
      ],
      "securityGroups": [
        "sg-xxxxxxxxx"
      ],
      "assignPublicIp": "ENABLED"
    }
  },
  "loadBalancers": [
    {
      "targetGroupArn": "arn:aws:elasticloadbalancing:ap-northeast-1:YOUR_ACCOUNT_ID:targetgroup/YOUR_TARGET_GROUP_NAME/xxxxxxxxx",
      "containerName": "shadcn-admin-container",
      "containerPort": 80
    }
  ],
  "serviceTags": [
    {
      "key": "Environment",
      "value": "production"
    },
    {
      "key": "Application",
      "value": "shadcn-admin"
    }
  ],
  "enableExecuteCommand": true,
  "deploymentConfiguration": {
    "maximumPercent": 200,
    "minimumHealthyPercent": 50,
    "deploymentCircuitBreaker": {
      "enable": true,
      "rollback": true
    }
  },
  "healthCheckGracePeriodSeconds": 60
}