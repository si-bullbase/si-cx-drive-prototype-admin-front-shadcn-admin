{
    "family": "cx-drive-prod-admin",
    "taskRoleArn": "arn:aws:iam::722479179779:role/cx-drive-prod-ecs-task-role",
    "executionRoleArn": "arn:aws:iam::722479179779:role/cx-drive-prod-ecs-task-execution-role",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "containerDefinitions": [
        {
            "name": "app",
            "image": "${APP_CONTAINER_IMAGE}:${APP_CONTAINER_TAG}",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "secrets": [],
            "environment": [
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "cx-drive-prod-admin",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "${APP_CONTAINER_TAG}"
                }
            }
        }
    ],
    "tags": []
}